# Vercel Environment Variables
# Copy these to your Vercel dashboard under Project Settings > Environment Variables

# Database Configuration
VERCEL_MONGO_URL=mongodb+srv://username:password@cluster.mongodb.net/ai_sales_girl?retryWrites=true&w=majority
VERCEL_MONGO_DB_NAME=ai_sales_girl
VERCEL_USE_MONGO=1

# Server Configuration
VERCEL_NODE_ENV=production

# Authentication
VERCEL_SESSION_SECRET=your-very-long-random-secret-key-here

# CORS Configuration
VERCEL_CORS_ORIGINS=https://your-domain.vercel.app

# Google OAuth (Optional)
VERCEL_GOOGLE_CLIENT_ID=your_google_client_id
VERCEL_GOOGLE_CLIENT_SECRET=your_google_client_secret

# VAPI Configuration (Optional)
VERCEL_VAPI_API_KEY=your_vapi_api_key
VERCEL_API_KEY=your_vapi_api_key
